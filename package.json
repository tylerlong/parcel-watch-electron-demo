{
  "name": "untitled-app",
  "scripts": {
    "lint": "eslint --fix '**/*.{ts,tsx,js,jsx}' && prettier --write .",
    "watch": "ts-node scripts/watch.ts",
    "start": "nodemon --watch build/electron.js --exec 'electron .'",
    "build": "rm -rf build && parcel build",
    "pack:mac": "yarn build && rm -rf dist && yarn electron-builder --universal --dir --config electron-builder.ts"
  },
  "main": "build/electron.js",
  "license": "UNLICENSED",
  "version": "0.1.0",
  "targets": {
    "electron": {
      "source": [
        "src/electron.ts",
        "src/preload.ts"
      ],
      "context": "electron-main",
      "distDir": "build"
    },
    "web": {
      "source": "src/index.html",
      "context": "browser",
      "distDir": "build",
      "publicUrl": "."
    }
  },
  "devDependencies": {
    "@tylerlong/use-proxy": "^1.2.3",
    "@types/node": "^18.15.3",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "antd": "^5.3.1",
    "electron": "^23.1.4",
    "electron-builder": "^24.0.0",
    "eslint": "^8.36.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "events": "^3.3.0",
    "nodemon": "^2.0.21",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shell-commands": "^0.1.2",
    "ts-node": "^10.9.1",
    "ttpt": "^0.3.2",
    "typescript": "^5.0.2",
    "yarn-upgrade-all": "^0.7.2"
  }
}
